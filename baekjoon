import math

while True:
    a = int(input())
    if a == 0:
        break
    else:
        a = str(a)
        # b = int(a)
        if len(a)%2 == 0:
            if a[0:math.floor(len(a)//2)] == reversed(a[math.ceil(len(a)//2):]):
                print("팰린드롬")
        if len(a)%2 == 1:
            pass
